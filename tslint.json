{
  "extends": "tslint:recommended",
  "rules": {
    "curly": false,
    "max-line-length": [
      false,
      120
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}